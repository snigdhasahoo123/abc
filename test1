public static void main(String[] args) {
		ArrayList<String> colors = new ArrayList<String>();
		colors.add("Brown");
		colors.add("Black");
		colors.add("Blue");
		colors.add("White");
		colors.add("Red");
		colors.add("Green");
		colors.add("Yellow");
		colors.add("Pink");
		colors.add("Violet");
		colors.add("purple");

		for (int i = 0; i < colors.size(); i++) {
			System.out.println(colors.get(i));
		}
	}
